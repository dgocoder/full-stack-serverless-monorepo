{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "js/apps/*",
    "js/packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.25.1"
}